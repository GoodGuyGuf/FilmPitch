<main>
  <% @projects.each do |project|%>
    <section>
      <h2><%= link_to project['name'], project_path(project['id']) %></h2>
      <p>Remaining: $<%=project['goal']%></p>
      <p><%=project['description']%></p>
      <p><%=project['film_type']%></p>
      <p>By: <%=project['author']%></p>
    </section>
  <% end %>
</main>