<main>
  <% @projects.each do |project|%>
    <section>
      <h2><%= link_to project['name'], project_path(project['id']) %></h2>
      <p>Created at: <%=Time.parse(project['created_at']).strftime("%a %m/%d/%y")%></p>
      <p>Remaining: $<%=project['goal']%></p>
      <p><%=project['description']%></p>
      <p><%=project['film_type']%></p>
    </section>
  <% end %>
</main>